# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'

target 'CryptoWatch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CryptoWatch
  pod 'SwiftyJSON'

end

target 'CryptoWatch WatchKit App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :watchos, '3.0'
  use_frameworks!

  # Pods for CryptoWatch WatchKit App
  pod 'SwiftyJSON'

end

target 'CryptoWatch WatchKit Extension' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :watchos, '3.0'
  use_frameworks!

  # Pods for CryptoWatch WatchKit Extension
  pod 'SwiftyJSON'

end
